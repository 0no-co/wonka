{
  "name": "wonka",
  "description": "A fast push & pull stream library for Reason/OCaml, Flow, and TypeScript",
  "version": "6.0.0",
  "author": "Phil Pluckthun <phil@kitten.sh>",
  "source": "./src/index.ts",
  "main": "./dist/wonka",
  "module": "./dist/wonka.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/wonka.mjs",
      "require": "./dist/wonka.js",
      "types": "./dist/types/index.d.ts",
      "source": "./src/index.ts"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "docs/*.md",
    "*.md"
  ],
  "keywords": [
    "wonka",
    "callbag",
    "callback",
    "observable",
    "iterable",
    "stream"
  ],
  "repository": "https://github.com/0no-co/wonka",
  "homepage": "https://wonka.kitten.sh",
  "bugs": {
    "url": "https://github.com/0no-co/wonka/issues"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.2",
    "jest": "^28.1.3",
    "typescript": "^4.7.4"
  }
}
