{
  "name": "wonka",
  "version": "0.1.0",
  "bsc-flags": ["-bs-super-errors", "-bs-no-version-header"],
  "refmt": 3,
  "package-specs": [
    {
      "module": "commonjs"
    },
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": [
        {
          "dir": "operators"
        },
        {
          "dir": "sources"
        },
        {
          "dir": "sinks"
        },
        {
          "dir": "web",
          "backend": ["js"]
        }
      ]
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "bs-dependencies" : [
    "bs-rebel"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "namespace": false
}
