{
  "name": "wonka",
  "version": "3.1.0",
  "namespace": false,
  "bsc-flags": ["-bs-super-errors", "-bs-no-version-header"],
  "refmt": 3,
  "package-specs": [
    {
      "module": "commonjs"
    },
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": [
        {
          "dir": "operators"
        },
        {
          "dir": "sources"
        },
        {
          "dir": "sinks"
        },
        {
          "dir": "web",
          "backend": ["js"]
        }
      ]
    },
    {
      "dir": "include",
      "subdirs": [
        {
          "dir": "rebel_native"
        },
        {
          "dir": "rebel_js",
          "backend": ["js"]
        }
      ]
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "bs-dependencies": [],
  "bs-dev-dependencies": ["@glennsl/bs-jest"]
}
